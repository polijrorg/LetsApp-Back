model Users {
  id                 String   @id @default(uuid())
  name               String
  email              String   @unique
  password           String
  defaultTransport   String   @default("Car")
  defaultFuel        String   @default("Gasoline")
  enableResoluteness Boolean  @default(true)
  created_at         DateTime @default(now())

  @@map("user")
}
